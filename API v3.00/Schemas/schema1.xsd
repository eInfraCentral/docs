<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://einfracentral.eu" elementFormDefault="qualified" targetNamespace="http://einfracentral.eu" version="1.0">

  <xs:import schemaLocation="schema2.xsd"/>

  <xs:element name="bundle" type="tns:bundle"/>

  <xs:element name="emailMessage" type="tns:emailMessage"/>

  <xs:element name="event" type="tns:event"/>

  <xs:element name="indicator" type="tns:indicator"/>

  <xs:element name="infraService" type="tns:infraService"/>

  <xs:element name="measurement" type="tns:measurement"/>

  <xs:element name="metadata" type="tns:metadata"/>

  <xs:element name="provider" type="tns:provider"/>

  <xs:element name="providerBundle" type="tns:providerBundle"/>

  <xs:element name="providerLocation" type="tns:providerLocation"/>

  <xs:element name="providerMainContact" type="tns:providerMainContact"/>

  <xs:element name="providerMerilDomain" type="tns:providerMerilDomain"/>

  <xs:element name="providerPublicContact" type="tns:providerPublicContact"/>

  <xs:element name="providerRequest" type="tns:providerRequest"/>

  <xs:element name="rangeValue" type="tns:rangeValue"/>

  <xs:element name="service" type="tns:service"/>

  <xs:element name="serviceCategory" type="tns:serviceCategory"/>

  <xs:element name="serviceMainContact" type="tns:serviceMainContact"/>

  <xs:element name="serviceProviderDomain" type="tns:serviceProviderDomain"/>

  <xs:element name="servicePublicContact" type="tns:servicePublicContact"/>

  <xs:element name="user" type="tns:user"/>

  <xs:element name="vocabulary" type="tns:vocabulary"/>

  <xs:complexType abstract="true" name="bundle">
    <xs:sequence>
      <xs:element name="active" type="xs:boolean"/>
      <xs:element minOccurs="0" ref="tns:metadata"/>
      <xs:element minOccurs="0" name="status" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="metadata">
    <xs:sequence>
      <xs:element default="null" minOccurs="0" name="modifiedAt" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="modifiedBy" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="originalId" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="registeredAt" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="registeredBy" type="xs:string"/>
      <xs:element default="null" minOccurs="0" name="source" type="xs:string"/>
      <xs:element minOccurs="0" name="terms">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="term" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="emailMessage">
    <xs:sequence>
      <xs:element minOccurs="0" name="message" type="xs:string"/>
      <xs:element minOccurs="0" name="recipientEmail" type="xs:string"/>
      <xs:element minOccurs="0" name="senderEmail" type="xs:string"/>
      <xs:element minOccurs="0" name="senderName" type="xs:string"/>
      <xs:element minOccurs="0" name="subject" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="event">
    <xs:sequence>
      <xs:element name="id" type="xs:string"/>
      <xs:element name="instant" type="xs:long"/>
      <xs:element name="service" type="xs:string"/>
      <xs:element name="type" type="xs:string"/>
      <xs:element name="user" type="xs:string"/>
      <xs:element minOccurs="0" name="value" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="extrasMapType">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="entry" nillable="true" type="tns:extrasType"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="extrasType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="key" type="xs:string"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="extrasMapTypeAdapter">
    <xs:complexContent>
      <xs:extension base="xmlAdapter">
        <xs:sequence/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="indicator">
    <xs:sequence>
      <xs:element name="description" type="xs:string"/>
      <xs:element minOccurs="0" name="dimensions">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" name="dimension" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="id" type="xs:string"/>
      <xs:element name="name" type="xs:string"/>
      <xs:element name="unit" type="xs:string"/>
      <xs:element name="unitName" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="infraService">
    <xs:complexContent>
      <xs:extension base="tns:bundle">
        <xs:sequence>
          <xs:element name="latest" type="xs:boolean"/>
          <xs:element minOccurs="0" ref="tns:service"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="service">
    <xs:sequence>
      <xs:element minOccurs="0" name="accessModes">
        <xs:annotation>
          <xs:documentation><![CDATA[Eligibility/criteria for granting access to users (excellence-based, free-conditionally, free etc.).]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="accessMode" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="accessPolicy" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Information about the access policies that apply.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="accessTypes">
        <xs:annotation>
          <xs:documentation><![CDATA[The way a user can access the service/resource (Remote, Physical, Virtual, etc.).]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="accessType" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="categories">
        <xs:annotation>
          <xs:documentation><![CDATA[A named group of Resources that offer access to the same type of Resources.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="category" type="tns:serviceCategory"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="certifications">
        <xs:annotation>
          <xs:documentation><![CDATA[List of certifications obtained for the Resource (including the certification body or URL if available).]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="certification" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="changeLog">
        <xs:annotation>
          <xs:documentation><![CDATA[Summary of the Resource features updated from the previous version.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="changeLog" nillable="true" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="description" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A high-level description in fairly non-technical terms of a) what the Resource does, functionality it provides and Resources it enables to access,
b) the benefit to a user/customer delivered by a Resource; benefits are usually related to alleviating pains
(e.g., eliminate undesired outcomes, obstacles or risks) or producing gains (e.g. increased performance, social gains, positive emotions or cost saving),
c) list of customers, communities, users, etc. using the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="fundingBody">
        <xs:annotation>
          <xs:documentation><![CDATA[Name of the funding body that supported the development and/or operation of the Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="fundingBody" nillable="true" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="fundingPrograms">
        <xs:annotation>
          <xs:documentation><![CDATA[Name of the funding program that supported the development and/or operation of the Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="fundingProgram" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="geographicalAvailabilities">
        <xs:annotation>
          <xs:documentation><![CDATA[Locations where the Resource is offered.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="geographicalAvailability" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="grantProjectNames">
        <xs:annotation>
          <xs:documentation><![CDATA[Name of the project that supported the development and/or operation of the Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="grantProjectName" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="helpdeskEmail" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The email to ask more information from the Provider about this Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="helpdeskPage" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[The URL to a webpage to ask more information from the Provider about this Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="id" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A persistent identifier, a unique reference to the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="languageAvailabilities">
        <xs:annotation>
          <xs:documentation><![CDATA[Languages of the (user interface of the) Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="languageAvailability" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="lastUpdate" type="xs:anySimpleType">
        <xs:annotation>
          <xs:documentation><![CDATA[Date of the latest update of the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="lifeCycleStatus" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Phase of the Resource life-cycle.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="logo" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to the logo/visual identity of the Resource. The logo will be visible at the Portal.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mainContact" type="tns:serviceMainContact">
        <xs:annotation>
          <xs:documentation><![CDATA[Service's Main Contact/Resource Owner info.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="maintenance" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage with information about planned maintenance windows for this Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="multimedia">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to video, screenshots or slides showing details of the Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="multimedia" nillable="true" type="xs:anyURI"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="name" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Brief and descriptive name of the Resource as assigned by the Provider.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="openSourceTechnologies">
        <xs:annotation>
          <xs:documentation><![CDATA[List of open source technologies supported by the Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="openSourceTechnology" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="order" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage through which an order for the Resource can be placed.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="orderType" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Information on the order type (requires an ordering procedure, or no ordering and if fully open or requires authentication).]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="paymentModel" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage with the supported payment models and restrictions that apply to each of them.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="pricing" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage with the information on the price scheme for this Resource in case the customer is charged for.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="privacyPolicy" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to the privacy policy applicable to the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="publicContacts">
        <xs:annotation>
          <xs:documentation><![CDATA[List of the Service's Public Contacts info.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="publicContact" type="tns:servicePublicContact"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="relatedPlatforms">
        <xs:annotation>
          <xs:documentation><![CDATA[List of suites or thematic platforms in which the Resource is engaged or Providers (Provider groups) contributing to this Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="relatedPlatform" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="relatedResources">
        <xs:annotation>
          <xs:documentation><![CDATA[List of other Resources that are commonly used with this Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="relatedResource" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="requiredResources">
        <xs:annotation>
          <xs:documentation><![CDATA[List of other Resources required to use this Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="requiredResource" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="resourceGeographicLocations">
        <xs:annotation>
          <xs:documentation><![CDATA[List of geographic locations where data is stored and processed.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="resourceGeographicLocation" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="resourceOrganisation" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The name (or abbreviation) of the organisation that manages or delivers the resource, or that coordinates resource delivery in a federated scenario.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="resourceProviders">
        <xs:annotation>
          <xs:documentation><![CDATA[The name(s) (or abbreviation(s)) of Provider(s) that manage or deliver the Resource in federated scenarios.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="resourceProvider" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="scientificDomains">
        <xs:annotation>
          <xs:documentation><![CDATA[The branch of science, scientific discipline that is related to the Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="scientificDomain" type="tns:serviceProviderDomain"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="securityContactEmail" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The email to contact the Provider for critical security issues about this Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="serviceLevel" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage with the information about the levels of performance that a Provider is expected to deliver.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="standards">
        <xs:annotation>
          <xs:documentation><![CDATA[List of standards supported by the Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="standard" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="statusMonitoring" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage with monitoring information about this Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="tagline" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Short catch-phrase for marketing and advertising purposes. It will be usually displayed close to the Resource name and should refer to the main value or purpose of the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="tags">
        <xs:annotation>
          <xs:documentation><![CDATA[Keywords associated to the Resource to simplify search by relevant keywords.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="tag" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="targetUsers">
        <xs:annotation>
          <xs:documentation><![CDATA[Type of users/customers that commissions a Provider to deliver a Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="targetUser" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="termsOfUse" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage describing the rules, Resource conditions and usage policy which one must agree to abide by in order to use the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="trainingInformation" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage to training information on the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="trl" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The Technology Readiness Level of the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="useCases">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to use cases supported by this Resource.]]></xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="useCase" type="xs:anyURI"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="userManual" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Link to the Resource user manual and documentation.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="version" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Version of the Resource that is in force.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="webpage" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation><![CDATA[Webpage with information about the Resource usually hosted and maintained by the Provider.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="serviceCategory">
    <xs:sequence>
      <xs:element name="category" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A named group of Resources that offer access to the same type of Resources]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="subcategory" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[A named group of Resources that offer access to the same type of Resource or capabilities, within the defined Resource Category.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="serviceMainContact">
    <xs:sequence>
      <xs:element name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Email of the Resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="firstName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[First Name of the Resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lastName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Last Name of the Resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="organisation" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The organisation to which the contact is affiliated.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="phone" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Telephone of the Resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="position" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Position of the Resource's main contact person/manager.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="servicePublicContact">
    <xs:sequence>
      <xs:element name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Email of the Resource's contact person or a generic email of the Provider to be displayed at the portal.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="firstName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[First Name of the Resource's contact person to be displayed at the portal.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="lastName" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Last Name of the Resource's contact person to be displayed at the portal.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="organisation" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The organisation to which the contact is affiliated.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="phone" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Telephone of the Resource's contact person to be displayed at the portal.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="position" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[Position of the Resource's contact person to be displayed at the portal.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="serviceProviderDomain">
    <xs:sequence>
      <xs:element name="scientificDomain" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The branch of science, scientific discipline that is related to the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="scientificSubdomain" type="xs:string">
        <xs:annotation>
          <xs:documentation><![CDATA[The subbranch of science, scientific subdicipline that is related to the Resource.]]></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="measurement">
    <xs:sequence>
      <xs:element name="id" type="xs:string"/>
      <xs:element name="indicatorId" type="xs:string"/>
      <xs:element minOccurs="0" name="locations">
